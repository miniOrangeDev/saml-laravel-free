{
  "name": "miniorange/saml-laravel-free",
  "description": "SAML authentication for SSO into Laravel Auth",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "miniOrange Security Software",
      "email": "info@miniorange.com"
    }
  ],
  "require": {
    "php": "^5.1 || ^7.1",
    "laravel/framework": ">=5.2"
  },
  "autoload": {
    "files": [
      "src/connector.php"
    ],
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "provider\\": "provider/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "provider\\ssoServiceProvider"
      ]
    }
  }
}
